import java.util.Scanner;

public class S11659 {
	public static void main(String args[]) {
		
		int N,M;
		int sum;
		int i,j;
		
		Scanner sc  = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		
		int[] nums = new int[N];
		
		for(int k=0; k<N; k++) {
			nums[k] = sc.nextInt();
		}
		
		for(int k=0; k<M; k++) {
			sum = 0;
			i = sc.nextInt();
			j = sc.nextInt();
			
			for(int l=i; l<=j; l++) {
				sum += nums[l];
			}
			System.out.println(sum);
		}
		
		sc.close();
		
	}

}
